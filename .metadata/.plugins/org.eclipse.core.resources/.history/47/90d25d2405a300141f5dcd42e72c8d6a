package com.acadgild.oops.encap;

class XYZ {
	private int a, b;

	void showdata() {
		System.out.print("a = " + getA() + "\nb = " + getB() + "\n");
	}

	public int getA() {
		return a;
	}

	public void setA(int a) {
		this.a = a;
	}

	public int getB() {
		return b;
	}

	public void setB(int b) {
		this.b = b;
	}
}
