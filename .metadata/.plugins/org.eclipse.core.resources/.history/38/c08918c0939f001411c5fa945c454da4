

public class Loopst {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1;i<100;i++){
			int j,count=0;
			boolean isPrime = true;
			for(j=1;j<=i;j++){
				int k = i%j;
		
				if(k==0){
					 isPrime = false;
					break;
					
					}
				
								
	
			}
			  // print the number
            if(isPrime)
                    System.out.print(i + " ");
			
    }
			
		}
				
}
