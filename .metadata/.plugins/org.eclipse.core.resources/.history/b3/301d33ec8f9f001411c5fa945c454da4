package loop;

public class looping {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int age=21;
		if (age >=18) {
			System.out.println("You are eligle for Voting");
			System.out.println(age%=3);
		} else {
			System.out.println("You are not Eligible");
		}	
	}
}
