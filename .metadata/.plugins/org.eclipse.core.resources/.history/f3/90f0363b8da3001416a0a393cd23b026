class SwitchDemo {
	public static void main(String[] args) {

		char value = 'i';

		switch (value) {
		case 'e':
			System.out.println("vowel");
			break;
		case 'i':
			System.out.println("vowel");
			break;
		case 'o':
			System.out.println("vowel");
			break;
		case 'u':
			System.out.println("vowel");
			break;
		case 'a':
			System.out.println("vowel");
			break;
		default:
			System.out.println("constants");
			break;
		}
	}
}
